{
  "name": "sheets",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "bun@1.2.21",
  "sideEffects": false,
  "scripts": {
    "dev": "vite --config vite.config.pages.ts",
    "build": "vite build",
    "build:pages": "vite build --config vite.config.pages.ts",
    "preview": "vite preview --config vite.config.pages.ts",
    "clean": "rimraf dist dist-pages",
    "lint": "eslint ./src",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ./src",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test:cov": "vitest run --coverage",
    "test": "vitest --run",
    "test:cell-editor": "vitest --run src/components/sheets/CellEditor.spec.tsx"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/coverage-v8": "^3.2.4",
    "bun-types": "^1.3.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^56.1.2",
    "jsdom": "^24.0.0",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@visx/glyph": "^3.12.0",
    "@visx/legend": "^3.12.0",
    "@visx/scale": "^3.12.0",
    "@visx/shape": "^3.12.0",
    "@visx/vendor": "^3.12.0",
    "react-icons": "^5.5.0",
    "react-router": "^7.9.4",
    "react-router-dom": "^7.9.4"
  }
}
